msgid "\"{segment}\" is a forbidden file or folder name."
msgstr "«{segment}» é un nome vedado para un ficheiro ou cartafol."

msgid "\"{segment}\" is a forbidden file type."
msgstr "«{segment}» é un tipo de ficheiro vedado."

msgid "\"{segment}\" is not allowed inside a file or folder name."
msgstr ""
"«{segment}» non está permitido dentro dun nome de ficheiro ou cartafol."

msgid "{count} file conflict"
msgid_plural "{count} files conflict"
msgstr[0] "{count} conflito de ficheiros"
msgstr[1] "{count} conflitos de ficheiros"

msgid "{count} file conflict in {dirname}"
msgid_plural "{count} file conflicts in {dirname}"
msgstr[0] "{count} conflito de ficheiros en {dirname}"
msgstr[1] "{count} conflitos de ficheiros en {dirname}"

msgid "{seconds} seconds left"
msgid_plural "{seconds} seconds left"
msgstr[0] "falta {seconds} segundo"
msgstr[1] "faltan {seconds} segundos"

