msgid "\"{segment}\" is a forbidden file or folder name."
msgstr "\"{segment}\" dosya ya da klasör adına izin verilmiyor."

msgid "\"{segment}\" is a forbidden file type."
msgstr "\"{segment}\" dosya türüne izin verilmiyor."

msgid "\"{segment}\" is not allowed inside a file or folder name."
msgstr "Bir dosya ya da klasör adında \"{segment}\" ifadesine izin verilmiyor."

msgid "{count} file conflict"
msgid_plural "{count} files conflict"
msgstr[0] "{count} dosya çakışması var"
msgstr[1] "{count} dosya çakışması var"

msgid "{count} file conflict in {dirname}"
msgid_plural "{count} file conflicts in {dirname}"
msgstr[0] "{dirname} klasöründe {count} dosya çakışması var"
msgstr[1] "{dirname} klasöründe {count} dosya çakışması var"

msgid "{seconds} seconds left"
msgid_plural "{seconds} seconds left"
msgstr[0] "{seconds} saniye kaldı"
msgstr[1] "{seconds} saniye kaldı"

#. TRANSLATORS time has the format 00:00:00
msgid "{time} left"
msgstr "{time} kaldı"

msgid "a few seconds left"
msgstr "bir kaç saniye kaldı"

msgid "assembling"
msgstr "birleştiriliyor"

msgid "Cancel"
msgstr "İptal"

