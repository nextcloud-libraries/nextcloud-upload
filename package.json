{
  "name": "@nextcloud/upload",
  "version": "1.0.0-beta.7",
  "description": "Nextcloud file upload client",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "author": "John Molakvo√¶ <skjnldsv@protonmail.com>",
  "license": "AGPL-3.0-or-later",
  "keywords": [
    "nextcloud",
    "webdav",
    "client",
    "upload",
    "chunk"
  ],
  "scripts": {
    "build": "rollup --config rollup.config.js",
    "build:doc": "typedoc --out dist/doc lib && touch dist/doc/.nojekyll",
    "dev": "echo 'No dev build available, production only' && npm run build",
    "watch": "rollup --config rollup.config.js --watch",
    "lint": "eslint --ext .js,.vue",
    "lint:fix": "eslint --ext .js,.vue --fix",
    "test": "jest",
    "test:watch": "jest --watchAll --verbose true",
    "test:coverage": "jest --coverage",
    "cypress": "cypress run --component",
    "cypress:gui": "cypress open --component",
    "l10n:extract": "node build/extract-l10n.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skjnldsv/nextcloud-upload.git"
  },
  "bugs": {
    "url": "https://github.com/skjnldsv/nextcloud-upload/issues"
  },
  "engines": {
    "node": "^16.0.0",
    "npm": "^7.0.0 || ^8.0.0"
  },
  "homepage": "https://github.com/skjnldsv/nextcloud-upload",
  "devDependencies": {
    "@cypress/vue2": "^1.0.2",
    "@nextcloud/babel-config": "^1.0.0",
    "@nextcloud/eslint-config": "^8.1.2",
    "@nextcloud/vue": "^5.4.0",
    "@nextcloud/webpack-vue-config": "^5.3.0",
    "@rollup-extras/plugin-clean": "^1.2.4",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.4.0",
    "@types/jest": "^28.1.8",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^10.6.0",
    "cypress-file-upload": "^5.0.8",
    "gettext-extractor": "^3.5.4",
    "gettext-parser": "^6.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^29.0.1",
    "rollup": "^2.78.1",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "rollup-plugin-styles": "^4.0.0",
    "rollup-plugin-vue2": "^0.8.1",
    "sass": "^1.54.6",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.3.1",
    "tslib": "^2.4.0",
    "typedoc": "^0.23.11",
    "typescript": "^4.8.2",
    "vue-template-compiler": "^2.7.10"
  },
  "dependencies": {
    "@nextcloud/auth": "^2.0.0",
    "@nextcloud/axios": "^2.0.0",
    "@nextcloud/files": "^3.0.0-beta.4",
    "@nextcloud/l10n": "^1.6.0",
    "@nextcloud/logger": "^2.3.0",
    "@nextcloud/router": "^2.0.0",
    "@nextcloud/vue": "^5.4.0",
    "@skjnldsv/sanitize-svg": "^1.0.2",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "p-cancelable": "^4.0.1",
    "p-limit": "^4.0.0",
    "p-queue": "^7.3.0",
    "simple-eta": "^3.0.2",
    "vue": "^2.7.10",
    "vue-material-design-icons": "^5.1.2"
  }
}
